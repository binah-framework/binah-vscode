{
  "name": "binah-syntax",
  "displayName": "Binah Syntax",
  "description": "Syntax highlighting for Binah files",
  "publisher": "nlehmann",
  "repository": {
    "type": "git",
    "url": "https://github.com/nilehmann/binah-syntax"
  },
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "binah",
        "aliases": [
          "Binah",
          "binah"
        ],
        "extensions": [
          ".binah"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "binah",
        "scopeName": "code.binah",
        "path": "./syntaxes/binah-spec.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^3.13.1"
  }
}
